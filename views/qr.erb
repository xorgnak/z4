<%= erb :partial_head %>

<div id='container'>
<h1 class='tag'><span><%= Z4.make(params[:user], :user).attr[:name] %></span></h1>
<h3 class='tag'><span><%= Z4.make(params[:chan], :chan).attr[:name] %></span></h3>
<div id='qrcode'></div>
</div>

<script>
$("#qrcode").qrcode('https://<%= request.host %>/u?user=<%= params[:user] %>&chan=<%= params[:chan] %>&epoch=<%= params[:epoch] %>');
</script>